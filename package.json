{
  "name": "flight-booker-frontend",
  "version": "1.0.0",
  "description": "Node.js proxy server for Flask backend.",
  "main": "server.js",
  "scripts": {
    "start": "node_modules/.bin/concurrently --kill-others-on-fail \"npm run flask\" \"npm run node\"",
    "dev": "node_modules/.bin/concurrently --kill-others-on-fail \"npm run flask\" \"npm run node\"",
    "node": "node server.js",
    "flask": "python3 Backend/app.py"
  },
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
