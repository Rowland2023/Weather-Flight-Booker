{
  "name": "node-flask-proxy",
  "version": "1.0.0",
  "description": "Node.js proxy server for Flask backend.",
  "main": "index.js",
  "scripts": {
    "start": "node index.js", 
    "dev": "concurrently --kill-others-on-fail \"npm run flask\" \"npm run node\"",
    "node": "node index.js",
    "flask": "cd ../Backend && python3 app.py"
  },
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}
