{
  "name": "flight-booker-frontend",
  "version": "1.0.0",
  "description": "Node gateway for Chrome Extension and Flask backend",
  "main": "server.js",
  "scripts": {
    "start": "concurrently \"npm run node\" \"npm run flask\"",
    "node": "cd Frontend && nodemon server.js",
    "flask": "cd Backend && flask --app app run --reload"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "nodemon": "^3.1.10"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "winston": "^3.18.1"
  }
}
